<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.wzc.g2_permissions">

    <uses-permission android:name="com.wzc.g2_permission_lib.PermissionActivity_ACCESS"/>
    <uses-permission android:name="com.wzc.g2_permission_lib.PermissionService_ACCESS"/>
    <uses-permission android:name="com.wzc.g2_permission_lib.PermissionBindService_ACCESS"/>
    <uses-permission android:name="com.wzc.g2_permission_lib.PermissionReceiver_ACCESS"/>

    <uses-permission android:name="com.wzc.g2_permission_lib.PermissionProvider_READ"/>
    <uses-permission android:name="com.wzc.g2_permission_lib.PermissionProvider_WRITE"/>

    <permission
        android:name="com.wzc.g2_permission.SendBroadcast_ACCESS"
        android:label="@string/sendbroadcast_permission_label"
        android:description="@string/sendbroadcast_permission_desc"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>